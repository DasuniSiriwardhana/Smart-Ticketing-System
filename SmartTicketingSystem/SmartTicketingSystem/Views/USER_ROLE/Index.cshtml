@model IEnumerable<SmartTicketingSystem.Models.USER_ROLE>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.roleID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.userID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AssignedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.member_id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.roleID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.userID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AssignedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.member_id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UserRoleID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UserRoleID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UserRoleID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
